bg-image($url)
  background-image: url($url + "@2x.png")
  @media (-webkit-min-device-pixel-ratio:3),(min-divice-pixel-raido:3)
    background-image: url($url + "@3x.png")
no-wrap()
  text-overflow: ellipsis
  overflow: hidden
  white-space: nowrap
extend-click()
  position: relative
  &:before
    content: ''
    position: absolute
    top: -10px
    right: -10px
    bottom: -10px
    left: -10px
fixed-cover()
  position: fixed
  left: 0
  top: 0
  right: 0
  bottom: 0
absolute-cover()
  position: absolute
  left: 0
  top: 0
  width: 100%
  height: 100%

